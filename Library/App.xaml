<Application x:Class="Library.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:Library"
             StartupUri="Start/AuthorizationWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\Colors.xaml"/>
                <ResourceDictionary Source="Styles\TextBlocks.xaml"/>
                <ResourceDictionary Source="Styles\Separatos.xaml"/>
                <ResourceDictionary Source="Styles\TextBoxs.xaml"/>
                <ResourceDictionary Source="Styles\PasswordBoxs.xaml"/>
                <ResourceDictionary Source="Styles\Buttons.xaml"/>
                <ResourceDictionary Source="Styles\ListViews.xaml"/>
                <ResourceDictionary Source="Styles\IconImages.xaml"/>
                <ResourceDictionary Source="Styles\DocumentViewers.xaml"/>
                <ResourceDictionary Source="Styles\ScrollViewers.xaml"/>
                <ResourceDictionary Source="Styles\ToolTips.xaml"/>
                <ResourceDictionary Source="Styles\ComboBoxes.xaml"/>


            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
